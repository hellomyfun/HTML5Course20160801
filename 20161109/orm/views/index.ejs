<!DOCTYPE html>
<html>
<head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css'/>
</head>
<body>
<h1><%= title %></h1>
<table border="1">
    <thead>
    <tr>
        <th>id</th>
        <th>用户名</th>
        <th>年龄</th>
    </tr>
    </thead>
    <tbody>
    <% for(var i = 0;i < users.length;i++){
        var user = users[i]; %>
    <tr>
        <td><%= user._id %></td>
        <td><%= user.name %></td>
        <td><%= user.age %></td>
    </tr>
    <% } %>
    </tbody>
</table>
</body>
</html>
